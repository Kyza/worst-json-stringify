{
	"compilerOptions": {
		"rootDir": "./",
		"baseUrl": "./",
		"outDir": "./dist/",
		"moduleResolution": "node",
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["DOM", "ESNext"],
		"declaration": true,
		"strict": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"paths": {
			"#wjs-dist": ["./dist/index.js"],
			"#wjs-dist/*": ["./dist/*"],
			"#wjs": ["./src/index.ts"],
			"#wjs/*": ["./src/*"]
		}
	}
}
