{
	"compilerOptions": {
		"rootDir": "./",
		"baseUrl": "./",
		"declarationMap": true,
		"outDir": "./dist/",
		"moduleResolution": "node",
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext"],
		"declaration": true,
		"strict": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"allowSyntheticDefaultImports": true,
		"paths": {
			"#*": ["./*"]
		}
	}
}
