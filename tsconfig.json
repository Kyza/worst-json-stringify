{
	"compilerOptions": {
		"declarationMap": true,
		"outDir": "./dist",
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["DOM", "ESNext"],
		"declaration": true,
		"strict": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"paths": {
			"#*": ["./*"]
		}
	},
	"include": ["src"]
}
