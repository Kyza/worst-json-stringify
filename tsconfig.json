{
	"compilerOptions": {
		"baseUrl": "./src/",
		"outDir": "./dist/",
		"moduleResolution": "node",
		"target": "ESNext",
		"lib": ["DOM", "ESNext"],
		"declaration": true,
		"strict": true,
		"paths": {
			"#wjs": ["./index.ts"],
			"#wjs/*": ["./*"]
		}
	},
	"include": ["src/**/*.ts"]
}
